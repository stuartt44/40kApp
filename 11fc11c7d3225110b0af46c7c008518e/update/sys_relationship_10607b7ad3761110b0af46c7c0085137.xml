        <insert_callback/>
        <name>Weapon Profiles</name>
        <query_from/>
        <query_with><![CDATA[(function refineQuery(current, parent) {
	var sourceUnit = parent.sys_id;
	var weapons = "";
	//Get wargear records
	var gr = new GlideRecord("x_903540_40k_arm_0_wargear_links");
	gr.addQuery("datasheet_id",sourceUnit);
	gr.query();
	//Add wargear records for query
	while (gr.next()) {
		weapons += gr.wargear_id +",";
	}
	//Query the profiles
	current.addQuery("wargear_id","IN",weapons);

})(current, parent);]]></query_with>
